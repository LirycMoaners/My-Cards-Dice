{
  "hosting": {
    "target": "hi-score",
    "public": "dist/hi-score",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": ".firebase/storage.rules"
  },
  "firestore": {
    "rules": ".firebase/firestore.rules",
    "indexes": ".firebase/firestore.indexes.json"
  }
}
